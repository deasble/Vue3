{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "parser": "@babel/eslint-parser"
    },
    "rules": {
        "no-unused-vars": "off",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto",
                "useTabs": false
            }
        ]
    }
}